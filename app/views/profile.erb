<div class="container">
  <div>
    <h2>Taken surveys:</h2>
    <ul>
      <% @taken_surveys.each do |survey| %>
        <li><%= survey.survey.title %></li>
      <% end %>
    </ul>
  </div>

  <div>
    <h2>Created surveys:</h2>
    <ul>
      <% @created_surveys.each do |survey| %>
        <li><%= survey.title %></li>
      <% end %>
    </ul>
  </div>

  <div>
    <h2>Surveys to Take:</h2>
    <ul>
      <% @all_surveys.each do |survey| %>
        <li><%= survey.title %></li>
      <% end %>
    </ul>
  </div>
</div>
